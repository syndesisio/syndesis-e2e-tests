{
  "name": "syndesis-e2e-tests",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "format": "yarn prettier -- --single-quote --print-width 120 --write \"./**/*.{ts,js}\"",
    "lint": "yarn ng lint",
    "lint:fix": "yarn ng lint -- --fix",
    "test:xvfb": "docker run --rm -v `pwd`:/syndesis-ui:z -w /syndesis-ui -u `id -u` docker.io/syndesis/karma-xvfb ./karma-xvfb.sh",
    "pree2e": "webdriver-manager update --versions.chrome 2.30",
    "e2e": "better-npm-run e2e:local",
    "pree2e:syndesis-qe": "webdriver-manager update --versions.chrome 2.30",
    "e2e:syndesis-qe": "better-npm-run e2e:syndesis-qe",
    "e2e:xvfb": "docker run -e SYNDESIS_UI_URL=$SYNDESIS_UI_URL --rm -v `pwd`:/syndesis-ui:z -w /syndesis-ui -u `id -u` docker.io/syndesis/karma-xvfb ./e2e-xvfb.sh",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --single-quote --print-width 120 --write",
      "git add"
    ]
  },
  "betterScripts": {
    "e2e:local": {
      "command": "protractor",
      "env": {
        "TEST_ENV": "local"
      }
    },
    "e2e:syndesis-qe": {
      "command": "protractor",
      "env": {
        "TEST_ENV": "syndesis-qe"
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "4.1.2",
    "@angular/compiler": "4.1.2",
    "@angular/core": "4.1.2",
    "better-npm-run": "^0.0.15",
    "core-js": "2.4.1",
    "es6-promise": "^4.1.0",
    "file-saver": "^1.3.3",
    "jquery": "3.1.1",
    "merge": "^1.2.0",
    "typescript-logging": "^0.2.1"
  },
  "devDependencies": {
    "@angular/cli": "1.0.3",
    "@angular/compiler-cli": "4.1.2",
    "@types/chai-as-promised": "^0.0.29",
    "@types/cucumber": "^0.0.37",
    "@types/file-saver": "0.0.0",
    "@types/fs-extra": "^3.0.3",
    "@types/jquery": "^2.0.41",
    "@types/lodash": "4.14.52",
    "@types/node": "7.0.5",
    "@types/protractor": "^4.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "codelyzer": "2.0.0",
    "cucumber": "^1.3.1",
    "cucumber-html-reporter": "^0.3.7",
    "cucumber-tsflow": "^1.1.2",
    "fs-extra": "^2.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "ncp": "^2.0.0",
    "prettier": "^1.5.3",
    "protractor": "5.1.1",
    "protractor-cucumber-framework": "^0.6.0",
    "ts-node": "2.1.0",
    "tslint": "^4.5.1",
    "typescript": "2.1.6",
    "webdriver-manager": "12.0.6"
  }
}
